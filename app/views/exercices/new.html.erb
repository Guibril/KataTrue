<h1>Exercices</h1>

<h2>Kata Garros</h2>

<%= form_for :exercices, url: exercices_path do |f| %>

   <p>
    <%= f.label :title, "titre"%><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :code, "code"%><br>
    <%= f.text_area :code %>
  </p>


  <p>
    <%= f.submit 'Soumettre' %>
  </p>
<% end %>
